module "webrtc_chime_starter_kit" {
  source = "github.com/agilityfeat/terraform-aws-webrtc-ventures-chime-starter-kit"

  name                                 = var.name
  aws_region                           = var.aws_region
  aws_account_id                       = var.aws_account_id
  aws_elb_account_id                   = var.aws_elb_account_id
  vpc_cidr                             = var.vpc_cidr
  vpc_name                             = var.vpc_name
  vpc_private_subnets                  = var.vpc_private_subnets
  vpc_public_subnets                   = var.vpc_public_subnets
  use_route53_hostedzone_for_acm       = var.use_route53_hostedzone_for_acm
  route53_hosted                       = var.route53_hosted
  app_domain                           = var.app_domain
  acm_unique_id                        = var.acm_unique_id
  acm_cert_arn                         = var.acm_cert_arn
  alb_unique_id                        = var.alb_unique_id
  alb_deregistration_delay             = var.alb_deregistration_delay
  alb_enable_deletion_protection       = var.alb_enable_deletion_protection
  alb_health_check_enabled             = var.alb_health_check_enabled
  alb_health_check_interval            = var.alb_health_check_interval
  alb_health_check_healthy_threshold   = var.alb_health_check_healthy_threshold
  alb_health_check_matcher             = var.alb_health_check_matcher
  alb_health_check_path                = var.alb_health_check_path
  alb_health_check_port                = var.alb_health_check_port
  alb_health_check_protocol            = var.alb_health_check_protocol
  alb_health_check_timeout             = var.alb_health_check_timeout
  alb_health_check_unhealthy_threshold = var.alb_health_check_unhealthy_threshold
  alb_idle_timeout                     = var.alb_idle_timeout
  alb_internal                         = var.alb_internal
  alb_ip_address_type                  = var.alb_ip_address_type
  alb_load_balancer_type               = var.alb_load_balancer_type
  alb_port                             = var.alb_port
  alb_preserve_host_header             = var.alb_preserve_host_header
  alb_target_type                      = var.alb_target_type
  alb_sec_grp_ingress                  = var.alb_sec_grp_ingress
  alb_sec_grp_egress                   = var.alb_sec_grp_egress
  asg_unique_id                        = var.asg_unique_id
  asg_ami_id                           = var.asg_ami_id
  asg_delete_on_termination            = var.asg_delete_on_termination
  asg_detailed_monitoring              = var.asg_detailed_monitoring
  asg_disk_size                        = var.asg_disk_size
  asg_health_check_grace_period        = var.asg_health_check_grace_period
  asg_health_check_type                = var.asg_health_check_type
  asg_instance_type                    = var.asg_instance_type
  asg_root_device                      = var.asg_root_device
  asg_size_configuration               = var.asg_size_configuration
  asg_scaling_alarms_config            = var.asg_scaling_alarms_config
  asg_scaling_policy                   = var.asg_scaling_policy
  asg_sec_grp_ingress                  = var.asg_sec_grp_ingress
  asg_sec_grp_egress                   = var.asg_sec_grp_egress
}
